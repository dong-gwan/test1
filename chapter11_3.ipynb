    "from tkinter import * \n",
    "alp = {}\n",
    "infilename = input(\"파일 이름을 입력하시오 : \")\n",
    "infile = open(infilename, \"r\")  \n",
    "rfile = infile.read()  \n",
    "infile.close()  \n",
    "\n",
    "for line in rfile:\n",
    "    for ch in line:\n",
    "        if ch in alp:\n",
    "            alp[ch] += 1\n",
    "        else:\n",
    "            alp[ch] = 1\n",
    "print(alp)"
   ]
